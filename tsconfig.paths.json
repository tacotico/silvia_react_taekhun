{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@pages": ["src/pages"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@store/*": ["src/store/*"],
      "@lib/*": ["src/lib/*"],
      "@assets": ["src/assets"],
